{
    "name": "andersondanilo/cnab_php",
    "type": "library",
    "description": "Projeto para criar arquivos de remessas e processar arquivos de retorno no formato CNAB",
    "keywords": ["boleto", "remessa", "retorno"],
    "licence": "MIT",
    "authors": [
        {
            "name": "Anderson Danilo",
            "email": "andersondanilo.souza@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/php-code-coverage": "1.2.*"
    },
    "require": {
        "php": ">=5.3.0",
        "mustangostang/spyc": "0.5.*"
    },
    "autoload": {
        "psr-4": {
            "Cnab\\": "src/Cnab"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "git submodule init",
            "git submodule update"
        ]
    }
}
